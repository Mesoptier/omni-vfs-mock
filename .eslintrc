{
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "indent": [2, 2],
    "strict": 0,
    "curly": [2, "multi-line"],
    "no-use-before-define": [2, "nofunc"],
    "no-underscore-dangle": 0,
    "constructor-super": 1,
    "no-this-before-super": 1,
    "object-shorthand": [2, "always"],
    "consistent-return": 0
  },
  "env": {
    "node": true,
    "es6": true
  }
}
